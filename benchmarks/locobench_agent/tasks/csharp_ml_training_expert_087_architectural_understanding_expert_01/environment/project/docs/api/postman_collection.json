{
  "info": {
    "name": "CanvasCraft ML Studio API",
    "description": "Postman collection that documents and exercises the RESTful endpoints exposed by the CanvasCraft ML Studio back-end.  The collection is organised by functional domainâ€”Datasets, Experiments, Models, Feature Store, Training Jobs, Monitoring, and Model Servingâ€”and demonstrates typical CRUD lifecycles, hyper-parameter tuning workflows, and model deployment pipelines.  \n\nEach request relies on two collection variables: `base_url` and `api_key`.  Provide the correct values in a Postman Environment or in the *Variables* tab before running the collection.\n\nBuilt for Postman Collection v2.1.",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "version": {
      "major": 1,
      "minor": 0,
      "patch": 0
    }
  },
  "variable": [
    {
      "key": "base_url",
      "value": "https://localhost:7071",
      "type": "string"
    },
    {
      "key": "api_key",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "ðŸ–¼ï¸ Datasets",
      "description": "Endpoints for manipulating raw and engineered datasets.",
      "item": [
        {
          "name": "List Datasets",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "X-Api-Key",
                "value": "{{api_key}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/datasets?take=25&skip=0",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "api",
                "v1",
                "datasets"
              ],
              "query": [
                { "key": "take", "value": "25" },
                { "key": "skip", "value": "0" }
              ]
            }
          },
          "response": [],
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status code is 200\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "pm.test(\"Result contains array\", function () {",
                  "    var json = pm.response.json();",
                  "    pm.expect(json.data).to.be.an('array');",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ]
        },
        {
          "name": "Create Dataset",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "X-Api-Key", "value": "{{api_key}}" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"ArtFaces-2024-Q1\",\n  \"description\": \"Portrait dataset consisting of 25k creative-commons images.\",\n  \"sourceUrl\": \"https://s3.example.com/artfaces.zip\",\n  \"tags\": [\"portraits\", \"faces\", \"2024\", \"creative-commons\"]\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/datasets",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "datasets"]
            }
          },
          "response": [],
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status is 201\", () => pm.response.to.have.status(201));",
                  "pm.collectionVariables.set(\"datasetId\", pm.response.json().id);"
                ],
                "type": "text/javascript"
              }
            }
          ]
        },
        {
          "name": "Upload Dataset File",
          "request": {
            "method": "PUT",
            "header": [
              { "key": "X-Api-Key", "value": "{{api_key}}" }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "file",
                  "type": "file",
                  "src": ""
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/api/v1/datasets/{{datasetId}}/upload",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "datasets", "{{datasetId}}", "upload"]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "ðŸŽ¨ Experiments",
      "description": "Track experiment metadata, metrics, artifact locations, and lineage.",
      "item": [
        {
          "name": "Create Experiment",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "X-Api-Key", "value": "{{api_key}}" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"GAN-Portraits-V3\",\n  \"datasetId\": \"{{datasetId}}\",\n  \"description\": \"Third iteration of GAN trained on the ArtFaces dataset.\",\n  \"parameters\": {\n    \"latent_dim\": 512,\n    \"learning_rate\": 0.0002,\n    \"batch_size\": 64\n  }\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/experiments",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "experiments"]
            }
          },
          "response": [],
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status 201\", () => pm.response.to.have.status(201));",
                  "pm.collectionVariables.set(\"experimentId\", pm.response.json().id);"
                ],
                "type": "text/javascript"
              }
            }
          ]
        },
        {
          "name": "Get Experiment Metrics",
          "request": {
            "method": "GET",
            "header": [
              { "key": "X-Api-Key", "value": "{{api_key}}" }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/experiments/{{experimentId}}/metrics",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "experiments", "{{experimentId}}", "metrics"]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "ðŸ§  Models",
      "description": "Model registry CRUD and version management endpoints.",
      "item": [
        {
          "name": "Register Model",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "X-Api-Key", "value": "{{api_key}}" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"experimentId\": \"{{experimentId}}\",\n  \"name\": \"GAN-Portraits-V3-Final\",\n  \"description\": \"Finalised GAN portrait generator.\",\n  \"stage\": \"Staging\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/models",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "models"]
            }
          },
          "response": [],
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status 201\", () => pm.response.to.have.status(201));",
                  "pm.collectionVariables.set(\"modelId\", pm.response.json().id);"
                ],
                "type": "text/javascript"
              }
            }
          ]
        },
        {
          "name": "Promote Model to Production",
          "request": {
            "method": "PATCH",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "X-Api-Key", "value": "{{api_key}}" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"stage\": \"Production\",\n  \"comment\": \"Performance validatedâ€”ready for production deployment.\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/models/{{modelId}}/stage",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "models", "{{modelId}}", "stage"]
            }
          },
          "response": []
        },
        {
          "name": "Rollback Model",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "X-Api-Key", "value": "{{api_key}}" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"targetVersion\": \"1.2.3\",\n  \"reason\": \"Observed drift in the most recent build. Rolling back.\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/models/{{modelId}}/rollback",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "models", "{{modelId}}", "rollback"]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "ðŸ§° Feature Store",
      "description": "Centralised repository for engineered features.",
      "item": [
        {
          "name": "List Features",
          "request": {
            "method": "GET",
            "header": [
              { "key": "X-Api-Key", "value": "{{api_key}}" }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/feature-store/features",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "feature-store", "features"]
            }
          },
          "response": []
        },
        {
          "name": "Add Feature",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "X-Api-Key", "value": "{{api_key}}" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"symmetry_score\",\n  \"type\": \"float\",\n  \"description\": \"Symmetry metric computed on facial landmarks.\",\n  \"transformation\": \"ComputeSymmetry(landmarks)\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/feature-store/features",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "feature-store", "features"]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "âš™ï¸ Training Jobs",
      "description": "Kick off asynchronous model training pipelines.",
      "item": [
        {
          "name": "Start Training Job",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "X-Api-Key", "value": "{{api_key}}" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"experimentId\": \"{{experimentId}}\",\n  \"computeTarget\": \"gpu-cluster-01\",\n  \"tags\": [\"nightly\", \"iter-3\"]\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/training-jobs",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "training-jobs"]
            }
          },
          "response": [],
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Accepted\", () => pm.response.to.have.status(202));",
                  "pm.collectionVariables.set(\"jobId\", pm.response.json().id);"
                ],
                "type": "text/javascript"
              }
            }
          ]
        },
        {
          "name": "Get Job Status",
          "request": {
            "method": "GET",
            "header": [
              { "key": "X-Api-Key", "value": "{{api_key}}" }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/training-jobs/{{jobId}}",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "training-jobs", "{{jobId}}"]
            }
          },
          "response": []
        },
        {
          "name": "Cancel Training Job",
          "request": {
            "method": "DELETE",
            "header": [
              { "key": "X-Api-Key", "value": "{{api_key}}" }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/training-jobs/{{jobId}}",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "training-jobs", "{{jobId}}"]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "ðŸ“ˆ Monitoring",
      "description": "Reactive drift and performance monitoring.",
      "item": [
        {
          "name": "List Alerts",
          "request": {
            "method": "GET",
            "header": [
              { "key": "X-Api-Key", "value": "{{api_key}}" }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/monitoring/alerts",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "monitoring", "alerts"]
            }
          },
          "response": []
        },
        {
          "name": "Acknowledge Alert",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "X-Api-Key", "value": "{{api_key}}" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"alertIds\": [\"{{alertId}}\"],\n  \"comment\": \"Triaged and accepted performance drift. Retuning model.\" \n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/monitoring/alerts/acknowledge",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "monitoring", "alerts", "acknowledge"]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "ðŸš€ Model Serving",
      "description": "Interact with live endpoints and send sample payloads.",
      "item": [
        {
          "name": "Predict (Sample Image)",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "multipart/form-data" },
              { "key": "X-Api-Key", "value": "{{api_key}}" }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "file",
                  "type": "file",
                  "src": ""
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/api/v1/serve/{{modelId}}/predict",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "serve", "{{modelId}}", "predict"]
            }
          },
          "response": []
        },
        {
          "name": "Feedback",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "X-Api-Key", "value": "{{api_key}}" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"predictionId\": \"{{predictionId}}\",\n  \"rating\": 4.5,\n  \"comment\": \"Great aesthetics, but colours slightly off\" \n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/serve/{{modelId}}/feedback",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "serve", "{{modelId}}", "feedback"]
            }
          },
          "response": []
        }
      ]
    }
  ]
}