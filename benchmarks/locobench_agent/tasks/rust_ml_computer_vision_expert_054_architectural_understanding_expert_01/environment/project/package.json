{
  "name": "@visutility/orchestrator",
  "displayName": "VisuTility Orchestrator",
  "version": "1.0.0",
  "description": "NodeJS tooling, WASM bindings, and developer productivity scripts for the Rust-based VisuTility Orchestrator (ml_computer_vision).",
  "license": "Apache-2.0",
  "homepage": "https://github.com/visutility/orchestrator#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/visutility/orchestrator.git"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/visutility"
  },
  "author": {
    "name": "VisuTility Core Team",
    "email": "opensource@visutility.ai",
    "url": "https://visutility.ai"
  },
  "contributors": [
    {
      "name": "Industrial AI Group",
      "url": "https://industrial-ai.org"
    }
  ],
  "keywords": [
    "computer-vision",
    "mlops",
    "rust",
    "wasm",
    "grpc",
    "orchestrator",
    "pipeline"
  ],
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.6.0"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "pkg",
    "proto",
    "LICENSE"
  ],
  "sideEffects": false,
  "bin": {
    "visutility-orchestrator": "./dist/cli.js"
  },
  "scripts": {
    "clean": "cargo clean && rimraf dist pkg coverage",
    "prebuild": "npm run clean",
    "build:wasm:dev": "wasm-pack build --debug --target web --out-dir pkg -- --features web",
    "build:wasm:release": "wasm-pack build --release --target web --out-dir pkg -- --features web",
    "build:node": "tsc --project tsconfig.json --outDir dist && rollup -c",
    "build": "npm-run-all build:*",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "proto:generate": "grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:./src/generated --js_out=import_style=commonjs,binary:./src/generated --grpc_out=grpc_js:./src/generated -I ./proto ./proto/*.proto",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky install",
    "commit": "cz",
    "release": "release-it",
    "start": "node --enable-source-maps dist/cli.js"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.10",
    "@grpc/proto-loader": "^0.7.8",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "node-addon-api": "^7.3.0",
    "pino": "^8.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/chalk": "^5.0.3",
    "@types/commander": "^11.0.0",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "grpc-tools": "^1.13.1",
    "grpc_tools_node_protoc_ts": "^5.3.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "release-it": "^17.0.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.5",
    "typescript": "^5.3.1",
    "vitest": "^0.34.6",
    "wasm-pack": "^0.12.1"
  },
  "optionalDependencies": {
    "@sentry/node": "^7.64.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "wasm-pack": {
      "outName": "visutility_orchestrator_wasm",
      "target": "web"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,md}": [
      "prettier --write"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): v${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "hooks": {
      "before:init": [
        "npm run lint",
        "npm test"
      ],
      "after:bump": "npm run build"
    }
  }
}