    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.22.3",
    "react-toastify": "^9.2.6",
    "stripe-js": "^1.68.0",
    "tailwindcss": "^3.4.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
