{
  "name": "temposcribe-pro-web-blog",
  "version": "1.0.0",
  "private": true,
  "description": "TempoScribe Pro â€“ Hexagonal Productivity Blog Suite (web_blog) front-end assets and build tooling.",
  "author": "TempoScribe Pro Core Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/temposcribe/temposcribe-pro.git"
  },
  "homepage": "https://temposcribe.io",
  "engines": {
    "node": ">=16.13.0",
    "npm": ">=8.1.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config webpack/webpack.dev.js --hot --progress",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "preview": "npm run build && serve -s dist",
    "analyze": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js --profile --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --max-warnings=0",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "postinstall": "node scripts/dotnet-manifest-sync.js",
    "start": "npm-run-all -p dev dotnet:watch",
    "dotnet:watch": "dotnet watch run --project ../TempoScribePro.Web/TempoScribePro.Web.csproj"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
