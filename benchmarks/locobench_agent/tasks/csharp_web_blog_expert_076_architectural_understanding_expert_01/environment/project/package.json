  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "resolutions": {
    "axios": "^1.6.7"
  }
