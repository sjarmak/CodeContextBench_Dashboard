```java
package com.wellsphere.connect.service.notification;

import android.app.Notification;
import android.app.NotificationChannel;
import android.app.NotificationManager;
import android.app.PendingIntent;
import android.app.TaskStackBuilder;
import android.content.Context;
import android.content.Intent;
import android.graphics.Color;
import android.os.Build;
import android.util.Base64;
import android.util.Log;

import androidx.annotation.NonNull;
import androidx.annotation.RequiresApi;
import androidx.core.app.NotificationCompat;

import com.google.firebase.crashlytics.FirebaseCrashlytics;
import com.google.firebase.messaging.FirebaseMessagingService;
import com.google.firebase.messaging.RemoteMessage;
import com.wellsphere.connect.R;
import com.wellsphere.connect.repository.NotificationRepository;
import com.wellsphere.connect.ui.MainActivity;

import java.nio.charset.StandardCharsets;
import java.security.GeneralSecurityException;
import java.security.Key;
import java.util.Map;

import javax.crypto.Cipher;
import javax.crypto.spec.SecretKeySpec;

/**
 * ConnectNotificationService is responsible for:
 *  1. Receiving push messages via Firebase Cloud Messaging.
 *  2. Forwarding the FCM token to the backend.
 *  3. Decrypting & parsing message payloads.
 *  4. Triggering foreground or background user-visible notifications.
 *
 *  NOTE: All PHI payload fields must be AES-encrypted in transit.
 */
public class ConnectNotificationService extends FirebaseMessagingService {

    private static final String TAG = "ConnectNotificationSvc";

    /* === Notification Channel Configuration === */
    private static final String CHANNEL_ID_HIGH       = "wellsphere.high.importance";
    private static final String CHANNEL_ID_DEFAULT    = "wellsphere.default.importance";
    private static final int    CHANNEL_IMPORTANCE_HIGH    = NotificationManager.IMPORTANCE_HIGH;
    private static final int    CHANNEL_IMPORTANCE_DEFAULT = NotificationManager.IMPORTANCE_DEFAULT;

    /* === Standard Extras === */
    private static final String EXTRA_ACTION = "action";
    private static final String EXTRA_TITLE  = "title";
    private static final String EXTRA_BODY   = "body";
    private static final String EXTRA_LEVEL  = "level";
    private static final String EXTRA_ENCRYPTED = "encrypted";

    /* === Security === */
    private static final String AES_ALGO = "AES/ECB/PKCS7Padding";
    // 256-bit pre-shared key stored in Android Keystore in real implementation.
    private static final byte[] DUMMY_KEY = "CHANGE_THIS_TO_REAL_KEY_32B".getBytes(StandardCharsets.UTF_8);

    /* === Repository === */
    private final NotificationRepository notificationRepository = NotificationRepository.getInstance();

    @Override
    public void onCreate() {
        super.onCreate();
        initNotificationChannels();
    }

    /**
     * Called whenever Firebase issues a new registration token. We forward the token
     * to our backend via NotificationRepository so that downstream services (SNS, APNS)
     * can correctly route push notifications.
     */
    @Override
    public void onNewToken(@NonNull String token) {
        super.onNewToken(token);
        Log.i(TAG, "Received new FCM token");

        // Persist token remotely (asynchronously)
        notificationRepository.registerDeviceToken(token)
                .doOnError(error -> {
                    Log.e(TAG, "Failed to register FCM token", error);
                    FirebaseCrashlytics.getInstance().recordException(error);
                })
                .subscribe(() -> Log.i(TAG, "FCM token registered successfully"));
    }

    /**
     * Primary entry point for push notifications while the app is in foreground/background.
     */
    @Override
    public void onMessageReceived(@NonNull RemoteMessage message) {
        super.onMessageReceived(message);
        Log.d(TAG, "FCM message received: " + message.getMessageId());

        final Map<String, String> data = message.getData();
        if (data == null || data.isEmpty()) {
            Log.w(TAG, "Message data payload empty. Ignoring.");
            return;
        }

        // Decrypt if required
        Map<String, String> processedData = data;
        if (Boolean.parseBoolean(data.get(EXTRA_ENCRYPTED))) {
            try {
                processedData = decryptPayload(data);
            } catch (GeneralSecurityException e) {
                FirebaseCrashlytics.getInstance().recordException(e);
                Log.e(TAG, "Unable to decrypt notification payload", e);
                return;
            }
        }

        // Build and issue notification
        buildAndShowNotification(processedData);
    }

    /* ============================================================================================
     * ===========                                     Private Helpers                          ====
     * ============================================================================================
     */

    /**
     * Creates Android notification channels for SDK 26+.
     */
    private void initNotificationChannels() {
        if (Build.VERSION.SDK_INT < Build.VERSION_CODES.O) return;

        NotificationManager manager = (NotificationManager) getSystemService(Context.NOTIFICATION_SERVICE);
        if (manager == null) return;

        NotificationChannel high = new NotificationChannel(
                CHANNEL_ID_HIGH,
                "Critical Alerts",
                CHANNEL_IMPORTANCE_HIGH);
        high.setDescription("High priority notifications that require immediate attention.");
        high.enableLights(true);
        high.setLightColor(Color.RED);
        high.enableVibration(true);

        NotificationChannel normal = new NotificationChannel(
                CHANNEL_ID_DEFAULT,
                "General Notifications",
                CHANNEL_IMPORTANCE_DEFAULT);
        normal.setDescription("Regular updates and informational messages.");
        normal.enableLights(false);
        normal.enableVibration(false);

        manager.createNotificationChannel(high);
        manager.createNotificationChannel(normal);
    }

    /**
     * Decrypts the provided message data using AES-256. The server Base64-encodes
     * each value after encryption to allow simple string transport.
     *
     * @throws GeneralSecurityException when decryption fails due to invalid key or payload.
     */
    @NonNull
    private Map<String, String> decryptPayload(@NonNull Map<String, String> encryptedData)
            throws GeneralSecurityException {

        Cipher cipher = Cipher.getInstance(AES_ALGO);
        Key key = new SecretKeySpec(DUMMY_KEY, "AES");
        cipher.init(Cipher.DECRYPT_MODE, key);

        for (Map.Entry<String, String> entry : encryptedData.entrySet()) {
            if (EXTRA_ENCRYPTED.equals(entry.getKey())) continue; // skip flag
            byte[] decoded = Base64.decode(entry.getValue(), Base64.DEFAULT);
            String decrypted = new String(cipher.doFinal(decoded), StandardCharsets.UTF_8);
            entry.setValue(decrypted);
        }
        return encryptedData;
    }

    /**
     * Decides which notification channel & style should be used, then shows it.
     */
    private void buildAndShowNotification(@NonNull Map<String, String> data) {

        String title = data.getOrDefault(EXTRA_TITLE, getString(R.string.app_name));
        String body  = data.getOrDefault(EXTRA_BODY, "");
        String action = data.get(EXTRA_ACTION);

        // Notification priority
        boolean isHigh = "critical".equalsIgnoreCase(data.get(EXTRA_LEVEL));
        String channelId = isHigh ? CHANNEL_ID_HIGH : CHANNEL_ID_DEFAULT;

        // Build pending intent leading to the correct in-app destination
        PendingIntent pendingIntent = buildPendingIntent(action);

        Notification notification = new NotificationCompat.Builder(this, channelId)
                .setSmallIcon(R.drawable.ic_notification_health)
                .setContentTitle(title)
                .setContentText(body)
                .setStyle(new NotificationCompat.BigTextStyle().bigText(body))
                .setColor(getResources().getColor(R.color.colorPrimary, null))
                .setAutoCancel(true)
                .setContentIntent(pendingIntent)
                .setPriority(isHigh ? NotificationCompat.PRIORITY_MAX : NotificationCompat.PRIORITY_DEFAULT)
                .build();

        NotificationManager manager = (NotificationManager) getSystemService(Context.NOTIFICATION_SERVICE);
        if (manager == null) return;

        int notificationId = (int) (System.currentTimeMillis() & 0xFFFFFFF);
        manager.notify(notificationId, notification);
    }

    /**
     * Creates a proper back-stack for deep-link navigation while ensuring that users are
     * sent to biometric auth gate if required.
     */
    private PendingIntent buildPendingIntent(String action) {

        Intent targetIntent;
        if ("open_journal".equalsIgnoreCase(action)) {
            targetIntent = new Intent(this, MainActivity.class)
                    .putExtra("destination", "journal");
        } else if ("open_messages".equalsIgnoreCase(action)) {
            targetIntent = new Intent(this, MainActivity.class)
                    .putExtra("destination", "messages");
        } else {
            // Default fallback â€“ open home
            targetIntent = new Intent(this, MainActivity.class);
        }

        // Build synthetic back stack
        TaskStackBuilder stackBuilder = TaskStackBuilder.create(this);
        stackBuilder.addNextIntentWithParentStack(targetIntent);

        int flags = Build.VERSION.SDK_INT >= Build.VERSION_CODES.S
                ? PendingIntent.FLAG_MUTABLE | PendingIntent.FLAG_UPDATE_CURRENT
                : PendingIntent.FLAG_UPDATE_CURRENT;

        return stackBuilder.getPendingIntent(0, flags);
    }
}
```