{
  "environment": {
    "type": "docker",
    "docker": {
      "image": "harbor-task:latest"
    }
  },
  "agents": [
    {
      "import_path": "agents.claude_sourcegraph_mcp_agent:ClaudeCodeSourcegraphMCPAgent",
      "model_name": "anthropic/claude-3-5-sonnet-20241022",
      "name": "claude-mcp"
    }
  ],
  "datasets": [
    {
      "path": "benchmarks/github_mined",
      "filter": {
        "task_names": ["sgt-001"]
      }
    }
  ],
  "execution": {
    "timeout_seconds": 600,
    "max_retries": 1
  }
}
